<resources>
    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false">org.lineageos.settings/org.lineageos.settings.doze.DozeSettingsActivity</string>

     <!-- Whether to show min refresh rate in display settings -->
    <bool name="config_show_min_refresh_rate_switch">true</bool>

    <!-- Whether to show peak refresh rate in display settings -->
    <bool name="config_show_peak_refresh_rate_switch">true</bool>

     <!-- Whether to show battery health -->
    <bool name="config_show_battery_health">true</bool>

    <!-- Define battery design capacity node -->
    <string name="config_battery_capacity_design">/sys/class/power_supply/bms/charge_full_design</string>

    <!-- Define battery now capacity node -->
    <string name="config_battery_capacity_now">/sys/class/power_supply/bms/charge_full</string>

    <!-- Define battery charge cycle node -->
    <string name="config_battery_charge_cycle">/sys/class/power_supply/bms/cycle_count</string>
</resources>
